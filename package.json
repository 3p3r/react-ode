{
  "name": "react-ode",
  "private": false,
  "version": "1.0.0",
  "description": "Embeddable online development environment in react",
  "scripts": {
    "start": "npm run storybook",
    "build": "webpack --config src/webpack/webpack.config.js",
    "test": "react-scripts test",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "peerDependencies": {
    "@emotion/react": "^11.0.0",
    "@emotion/styled": "^11.0.0",
    "@monaco-editor/react": "^4.0.0",
    "@mui/material": "^5.0.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "xterm": "^4.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.18.0",
    "@emotion/babel-preset-css-prop": "^11.2.0",
    "@emotion/react": "^11.9.0",
    "@emotion/styled": "^11.8.1",
    "@monaco-editor/react": "^4.4.5",
    "@mui/icons-material": "^5.8.0",
    "@mui/lab": "^5.0.0-alpha.83",
    "@mui/material": "^5.8.1",
    "@storybook/addon-actions": "^6.5.6",
    "@storybook/addon-essentials": "^6.5.6",
    "@storybook/addon-interactions": "^6.5.6",
    "@storybook/addon-links": "^6.5.6",
    "@storybook/builder-webpack5": "^6.5.6",
    "@storybook/manager-webpack5": "^6.5.6",
    "@storybook/preset-create-react-app": "^4.1.1",
    "@storybook/react": "^6.5.6",
    "@storybook/testing-library": "^0.0.11",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.2.0",
    "@testing-library/user-event": "^13.5.0",
    "babel-loader": "^8.2.5",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^11.0.0",
    "isomorphic-git": "^1.17.2",
    "lodash": "^4.17.21",
    "re-resizable": "^6.9.9",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-ode-cash-money": "^1.2.2",
    "react-resize-detector": "^7.1.1",
    "react-scripts": "5.0.1",
    "storybook-dark-mode": "^1.1.0",
    "web-vitals": "^2.1.4",
    "webpack": "^5.72.1",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.9.0",
    "webpack-node-externals": "^3.0.0",
    "xterm": "^4.18.0",
    "xterm-addon-fit": "^0.5.0",
    "xterm-addon-web-links": "^0.5.1",
    "xterm-addon-webgl": "^0.11.4",
    "xterm-js-shell": "^1.1.3"
  }
}
